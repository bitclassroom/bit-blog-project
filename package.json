{
    "name": "bit-show-react",
    "version": "1.0.0",
    "description": "BIT Blog SPA - Building a React Based Electron App",
    "private": true,
    "author": "Marko Arsic <marsicdev@gmail.com>",
    "main": "public/electron.js",
    "homepage": "./",
    "license": "MIT",
    "build": {
        "appId": "io.hypetech.blog.app"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "electron": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
        "eject": "react-scripts eject",
        "precommit": "pretty-quick --staged",
        "clean": "rm -rf node_modules/ && yarn cache clean",
        "reset": "rm -rf node_modules/ && yarn cache clean && yarn"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "electron-is-dev": "^2.0.0",
        "lodash": "^4.17.21",
        "materialize-css": "^1.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-images": "^1.1.7",
        "react-router-dom": "^5.2.0",
        "react-slick": "^0.28.1",
        "slick-carousel": "^1.8.1"
    },
    "devDependencies": {
        "concurrently": "^6.0.2",
        "cross-env": "^7.0.3",
        "electron": "^12.0.5",
        "electron-builder": "^22.10.5",
        "electron-devtools-installer": "^3.2.0",
        "husky": "^6.0.0",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "react-scripts": "4.0.3",
        "wait-on": "^5.3.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
